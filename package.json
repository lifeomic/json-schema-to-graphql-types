{
  "name": "json-schema-to-graphql-types",
  "version": "1.0.0",
  "description": "A utility to convert JSON Schema types into GraphQL types",
  "bin": {
    "convert-json-schemas-to-graphql-types": "bin/convert-json-schemas-to-graphql-types.js"
  },
  "scripts": {
    "lint": "eslint .",
    "test": "nyc ava",
    "posttest": "yarn lint",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "engines": {
    "node": ">=8"
  },
  "keywords": [
    "graphql",
    "json-schema",
    "converter"
  ],
  "author": "Matt Lavin <matt.lavin@lifeomic.com>",
  "license": "MIT",
  "dependencies": {
    "camelcase": "^5.0.0",
    "escodegen": "^1.9.1",
    "fs-extra": "^6.0.0",
    "graphql": "^0.13.0",
    "lodash": "^4.17.5",
    "uppercamelcase": "^3.0.0",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "ajv": "^6.2.0",
    "ava": "^0.25.0",
    "coveralls": "^3.0.0",
    "eslint": "^4.18.1",
    "eslint-config-semistandard": "^12.0.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-lodash": "^2.6.1",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "nyc": "^11.4.1",
    "tmp-promise": "^1.0.4"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100
  }
}
